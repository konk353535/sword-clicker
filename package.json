{
  "name": "sword-clicker",
  "private": true,
  "scripts": {
    "placeholder": "MONGO_URL=mongodb://admin:admin@127.0.0.1:27017/test?replicaSet=dbrs MONGO_OPLOG_URL=mongodb://oplogger:oplogger@127.0.0.1:27017/local?replicaSet=dbrs&authSource=admin",
    "dev": "CLUSTER_WORKERS_COUNT=1 meteor run --settings settings-development.json",
    "test": "meteor test --once --driver-package meteortesting:mocha",
    "test-app": "TEST_WATCH=1 meteor test --full-app --driver-package meteortesting:mocha",
    "visualize": "meteor --production --extra-packages bundle-visualizer",
    "format": "prettier --write \"**/*.{js,ts,tsx,md}\"",
    "colorize": "tsx ./dev/colorize.ts"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:import/typescript",
      "prettier"
    ],
    "rules": {
      "no-void": "off",
      "@typescript-eslint/no-dynamic-delete": "off",
      "@typescript-eslint/no-non-null-assertion": "off",
      "@typescript-eslint/no-unused-vars": "off",
      "@typescript-eslint/restrict-template-expressions": "off"
    },
    "root": true
  },
  "dependencies": {
    "@babel/runtime": "^7.20.7",
    "@fancyapps/fancybox": "^3.5.7",
    "@opentelemetry/api": "^1.6.0",
    "@opentelemetry/auto-instrumentations-node": "^0.39.2",
    "@opentelemetry/context-zone": "^1.17.0",
    "@opentelemetry/exporter-collector": "^0.25.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.43.0",
    "@opentelemetry/instrumentation": "^0.43.0",
    "@opentelemetry/instrumentation-fetch": "^0.43.0",
    "@opentelemetry/resources": "^1.17.0",
    "@opentelemetry/sdk-metrics": "^1.17.0",
    "@opentelemetry/sdk-node": "^0.43.0",
    "@opentelemetry/sdk-trace-base": "^1.17.0",
    "@opentelemetry/sdk-trace-web": "^1.17.0",
    "animate.css": "^3.5.2",
    "arrowpay-react-checkout": "^1.3.1",
    "async": "^2.4.1",
    "babel-cli": "^6.26.0",
    "babel-preset-node6": "^11.0.0",
    "babel-runtime": "^6.20.0",
    "bootstrap": "^4.0.0-alpha.6",
    "bufferutil": "^4.0.7",
    "core-js": "^2.5.7",
    "envalid": "^7.3.1",
    "faker": "^4.1.0",
    "jquery": "^3.7.1",
    "jquery-typeahead": "^2.11.1",
    "js-object-parser": "^1.0.1",
    "json-parser": "^1.1.5",
    "jsonic": "^0.3.0",
    "lodash": "^4.17.11",
    "mathjs": "^3.13.3",
    "meteor-node-stubs": "^1.2.5",
    "moment": "^2.22.2",
    "node-uuid": "^1.4.8",
    "numeral": "^2.0.6",
    "parse-json": "^2.2.0",
    "prom-client": "^14.2.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "simpl-schema": "^3.4.1",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "stripe": "^4.17.1",
    "tsx": "^3.12.10",
    "underscore": "^1.9.1",
    "utf-8-validate": "^6.0.3",
    "weighted-random": "^0.1.0"
  },
  "devDependencies": {
    "@types/mocha": "^8.2.3",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-node6": "^11.0.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.1.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.2.3",
    "typescript": "^5.2.2"
  },
  "meteor": {
    "mainModule": {
      "client": "client/main.js",
      "server": "server/main.js"
    }
  }
}
